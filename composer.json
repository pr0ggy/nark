{
    "name": "pr0ggy/phaser",
    "description": "A spying/stubbing library for PHP",
    "type": "library",
    "keywords": ["php", "unit", "testing", "stub", "stubbing", "spy", "spying", "library"],
    "license": "GPL",
    "require": {
        "php": ">=5.6.0",
        "equip/structure": "^1.1",
        "hamcrest/hamcrest-php": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4" : {
            "Phaser\\": "./lib/classes"
        },
        "files": [
            "./loader.php"
        ]
    },
    "autoload-dev": {
        "psr-4" : {
            "Phaser\\": "./test",
            "Phaser\\Fixtures\\": "./test/fixtures"
        }
    }
}
